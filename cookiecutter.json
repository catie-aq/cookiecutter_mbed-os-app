{
   "project_name": "Name of the project",
   "project_short_description": "A short description of the project.",
   "project_slug": "{{ cookiecutter.project_name | slugify }}",
   "git_url": "https://github.com/catie-aq/{{cookiecutter.project_slug}}.git",
   "copyright_holder": "CATIE",
   "copyright_year": "{% now 'utc', '%Y' %}",
   "open_source_license": ["Apache Software License 2.0", "Not open source"],
   "custom_target": [
      "None",
      "Z_Environment",
      "Z_Motion",
      "Zest_Core_FMLR-72",
      "Zest_Core_MTXDOT",
      "Zest_Core_nRF52832",
      "Zest_Core_STM32G474VE",
      "Zest_Core_STM32H743ZG",
      "Zest_Core_STM32H753ZI",
      "Zest_Core_STM32L496RG",
      "Zest_Core_STM32L4A6RG",
      "Zest_Core_STM32L562VE"
   ],
   "_custom_target_repo": "{{ cookiecutter.custom_target | custom_target_repo }}",
   "_custom_target_repo_url": "{{ cookiecutter.custom_target | custom_target_repo_url }}",
   "_mbed_os_target": "{{ cookiecutter.custom_target | mbed_os_target }}",
   "_jlink_device": "{{ cookiecutter.custom_target | jlink_device }}",
   "toolchain": "GCC_ARM",
   "create_repository": [
      true,
      false
   ],
   "_extensions": [
      "jinja2_time.TimeExtension",
      "local_extensions.CustomTargetExtension"
   ]
}
